{
  "hosting": {
      "firestore": {
        "rules": "firestore.rules"
      },
      "storage": {
        "rules": "storage.rules"
      },
      "hosting": {
        "public": "./public"
      },
    "public": "./public",
    "headers": [{
      "source" : "**/*.@(js|html)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=0"
      } ]
    }]
  }
}